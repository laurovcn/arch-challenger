generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Students {
  id         Int       @id @default(autoincrement())
  n1         String
  n2         String
  n3         String
  n4         String
  teachers   Teachers? @relation(fields: [teachersId], references: [id])
  teachersId Int?
}

model Teachers {
  id       Int        @id @default(autoincrement())
  name     String
  students Students[]
}
